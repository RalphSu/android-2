<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/NavPage"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/result_tv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="@string/enter"
        android:textSize="20sp" />

    <Button
        android:id="@+id/umeng_example_analytics_online_config"
        style="@style/MMLineActionButton"
        android:onClick="onButtonClick"
        android:text="@string/online_config" >
    </Button>

    <Button
        android:id="@+id/umeng_example_analytics_event"
        style="@style/MMLineActionButton"
        android:onClick="onButtonClick"
        android:text="@string/um_event" >
    </Button>

    <Button
        android:id="@+id/umeng_example_analytics_ekv"
        style="@style/MMLineActionButton"
        android:onClick="onButtonClick"
        android:text="@string/um_event2" >
    </Button>
    <Button
        android:id="@+id/umeng_example_analytics_event_begin"
        style="@style/MMLineActionButton"
        android:onClick="onButtonClick"
        android:text="@string/um_event_start" >
    </Button>
    <Button
        android:id="@+id/umeng_example_analytics_event_end"
        style="@style/MMLineActionButton"
        android:onClick="onButtonClick"
        android:text="@string/um_event_end" >
    </Button>
    
</LinearLayout>